<div class="title">My Books</div>

<div class="book-list">
<% @books.each do |book| %>   
    <div class="books">
          <h2><%= book.title %></h2>
          <h3><%= book.author %></h3>
        <a class="details" href="/books/<%= book.id %>">
          Details >
        </a>
    </div> 
  <% end %>
</div>